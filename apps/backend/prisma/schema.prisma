generator client {
  provider = "prisma-client"
  output   = "../src/modules/prisma/generated"
}

datasource db {
  provider = "postgresql"
}

model iss_log {
  id Int @id @default(autoincrement())

  fetchedAt DateTime @default(now()) @map("fetched_at")

  sourceUrl String

  payload Json

  @@map("iss_logs")
}

model space_cache {
  id Int @id @default(autoincrement())

  fetchedAt DateTime @default(now()) @map("fetched_at")

  source String

  payload Json

  @@map("space_cache")
}