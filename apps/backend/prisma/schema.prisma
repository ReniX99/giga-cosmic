generator client {
  provider = "prisma-client"
  output   = "../src/modules/prisma/generated"
}

datasource db {
  provider = "postgresql"
}

model iss_log {
  id Int @id @default(autoincrement())

  fetchedAt DateTime @default(now()) @map("fetched_at")

  sourceUrl String

  payload Json

  @@map("iss_logs")
}

model space_cache {
  id Int @id @default(autoincrement())

  fetchedAt DateTime @default(now()) @map("fetched_at")

  source String

  payload Json

  @@map("space_cache")
}

model osdr_item {
  id Int @id @default(autoincrement())

  datasetId String @map("dataset_id")

  title String

  status String

  updatedAt DateTime @map("updated_at")

  insertedAt DateTime @default(now()) @map("inserted_at")

  raw Json

  @@map("osdr_items")
}